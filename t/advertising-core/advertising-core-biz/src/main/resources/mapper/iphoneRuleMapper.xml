<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xtzn.core.mapper.IIphoneRuleMapper">

    <select id="selectUrl" resultType="java.util.Map">
    select NAME name,URL url from iphone_rule where STATUS = 1 and USER_ID = #{userId,jdbcType=INTEGER}
  </select>
</mapper>  